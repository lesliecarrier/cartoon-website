$body-color: #FEFEFE
$banner-color: #FEFEFE
$container-color: #FEFEFE
$header-color: #674172
$website-font: "dinosaursonar"

.container-fluid, header
  background-color: $banner-color

.container
  background: $container-color
  min-height: 700px

body
  background-color: $body-color
  font-family: $website-font
  font-size: 30px

div.cartoonContainer
  max-width: 900px

header
  padding: 15px 0px

header>a>h1
  color: $header-color

h1
  font-family: $website-font
  font-size: 50px
